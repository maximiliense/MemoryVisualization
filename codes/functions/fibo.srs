fn main() {
    let n: i32 = 4;
    let f = fibonacci(n);
    print!("Fibo = ");
    println!(f);
    return;
}
fn fibonacci(n: i32) {
    let res: i32;
    if n == 1 {
        res = 1;
    } else {
        if n == 2 {
            res = 1;
        } else {
            n -= 1;
            let f1: i32 = fibonacci(n);
            n -= 1;
            let f2: i32 = fibonacci(n);
            res = f1 + f2;
        }
    }
    return res;
}
